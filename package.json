{
  "name": "ReactNativeNavigationStarterKit",
  "version": "1.0.0",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "lint:ts": "node node_modules/.bin/tslint src/**/*.ts{,x}",
    "lint:css": "node node_modules/.bin/stylelint ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts{,x}": [
      "node node_modules/.bin/tslint src/**/*.ts{,x} --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.5.0",
    "formik": "1.5.7",
    "native-base": "2.12.1",
    "react": "16.8.6",
    "react-native": "0.59.9",
    "react-native-navigation": "2.22.1",
    "react-redux": "7.1.0",
    "redux": "4.0.1",
    "styled-components": "4.3.2",
    "styled-is": "1.3.0",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/runtime": "7.4.5",
    "@strv/stylelint-config-styled-components": "1.1.1",
    "@types/jest": "24.0.15",
    "@types/ramda": "0.26.13",
    "@types/react": "16.8.22",
    "@types/react-native": "0.57.65",
    "@types/react-native-navigation": "2.0.0",
    "@types/react-redux": "7.1.1",
    "@types/react-test-renderer": "16.8.2",
    "@types/styled-components": "4.1.16",
    "@types/yup": "0.26.19",
    "babel-jest": "24.8.0",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "lint-staged": "8.2.1",
    "metro-react-native-babel-preset": "0.55.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.8.6",
    "stylelint": "10.1.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "greenkeeper": {
    "ignore": [
      "react",
      "react-native",
      "react-native-navigation"
    ]
  },
  "keywords": [
    "formik",
    "native-base",
    "react",
    "hooks",
    "react-hooks",
    "react-native",
    "react-native-navigation",
    "wix",
    "redux",
    "react-redux",
    "recompose",
    "styled-components",
    "stylelint",
    "eslint",
    "styled-is",
    "yup",
    "jest",
    "react-test-renderer",
    "authorization"
  ]
}
