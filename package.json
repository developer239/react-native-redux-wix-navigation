{
  "name": "ReactNativeNavigationStarterKit",
  "version": "1.0.0",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "lint:ts": "node node_modules/.bin/tslint src/**/*.ts{,x}",
    "lint:css": "node node_modules/.bin/stylelint ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts{,x}": [
      "node node_modules/.bin/tslint src/**/*.ts{,x} --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.2.1",
    "formik": "^1.5.1",
    "native-base": "^2.12.1",
    "react": "16.8.3",
    "react-native": "0.59.1",
    "react-native-navigation": "^2.13.1",
    "react-redux": "^7.0.0",
    "redux": "^4.0.1",
    "styled-components": "^4.1.3",
    "styled-is": "^1.2.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@strv/stylelint-config-styled-components": "^1.0.0",
    "@types/jest": "^24.0.11",
    "@types/ramda": "^0.26.3",
    "@types/react": "^16.8.8",
    "@types/react-native": "^0.57.40",
    "@types/react-native-navigation": "^2.0.0",
    "@types/react-redux": "^7.0.3",
    "@types/react-test-renderer": "^16.8.1",
    "@types/styled-components": "^4.1.12",
    "@types/yup": "^0.26.12",
    "babel-jest": "^24.5.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.4",
    "stylelint": "^9.10.1",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "preset": "react-native"
  },
  "greenkeeper": {
    "ignore": [
      "react",
      "react-native",
      "react-native-navigation"
    ]
  },
  "keywords": [
    "formik",
    "native-base",
    "react",
    "hooks",
    "react-hooks",
    "react-native",
    "react-native-navigation",
    "wix",
    "redux",
    "react-redux",
    "recompose",
    "styled-components",
    "stylelint",
    "eslint",
    "styled-is",
    "yup",
    "jest",
    "react-test-renderer",
    "authorization"
  ]
}
